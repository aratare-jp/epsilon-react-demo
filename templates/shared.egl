[%
operation t_attribute getTsType(): String {
	var type = self.a_type;
	if (type.isUndefined()) {
		return "string";
	}
	var typeMap = new Map();
	typeMap.put("string", "string");
	typeMap.put("integer", "number");
	return typeMap.get(type);
}

operation t_attribute getInputType(): String {
	var type = self.a_type;
	if (type.isUndefined()) {
		return "text";
	}
	var typeMap = new Map();
	typeMap.put("string", "text");
	typeMap.put("integer", "number");
	return typeMap.get(type);
}
%]